Option Explicit
Private Sub cmdClear_Click()
    Dim ctl As Control
    For Each ctl In Me.Controls
    Select Case ctl.ControlType
    Case acCheckBox
        ctl.Value = False
    Case acTextBox
        ctl.Value = Null
    End Select
    Next ctl
End Sub
Private Sub cmdCancel_Click()
    DoCmd.Close acForm, "frmFacilitators"
End Sub
Private Sub Form_Open(Cancel As Integer)
    Dim ctl As Control
    Dim strFacilitators As String
    Dim facsArray As Variant
    strFacilitators = Nz(Forms!frmProgram![sbfrmActivitySchedule].Form.txtStaffFacilitators.Value)
    Me.txtFacilitators = strFacilitators
    
    If IsNull(Me.txtFacilitators) = False And Me.txtFacilitators <> "" Then
        strFacilitators = Me.txtFacilitators.Value
        facsArray = Split(strFacilitators, "'")
        For Each ctl In Me.Controls
            Select Case ctl.ControlType
            Case acCheckBox
            If IsInArray(ctl.tag, facsArray) Then
                ctl.Value = True
            End If
            End Select
        Next ctl
    End If
End Sub
Private Sub cmdDone_Click()
    Forms![frmProgram]![sbfrmActivitySchedule].Form![txtStaffFacilitators].Value = (Forms![frmFacilitators]![txtFacilitators].Value)
    DoCmd.Close acForm, "frmFacilitators"
End Sub
