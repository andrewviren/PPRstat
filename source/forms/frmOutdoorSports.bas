Private Sub cmdGoToNext_Click()
    Dim Db As DAO.Database
    Dim rs As Recordset
    
    Set Db = CurrentDb()
    Set rs = Me.RecordsetClone
    
    If Me.CurrentRecord <> rs.RecordCount Then
        DoCmd.GoToRecord , , acNext
    End If
  
    rs.Close
    
End Sub
