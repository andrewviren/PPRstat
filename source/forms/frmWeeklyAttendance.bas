Private Sub cmdCloseApp_Click()
On Error GoTo Err_cmdCloseApp_Click

    If Me.Dirty Then Me.Dirty = False
    DoCmd.Quit

Exit_cmdCloseApp_Click:
    Exit Sub

Err_cmdCloseApp_Click:
    MsgBox Err.Description
    Resume Exit_cmdCloseApp_Click
    
End Sub
Private Sub txtProgID_find_AfterUpdate()
Dim rst As DAO.Recordset
Dim strCriteria As String

strCriteria = "[ProgramID] =" & Me.txtProgID_find
Set rst = Me.RecordsetClone

    
    If IsNull(Me.txtProgID_find) Or Me.txtProgID_find = "" Or (IsNumeric(Me.txtProgID_find) = False) Then
        MsgBox "Please provide numeric ID", vbOKOnly, "ID needed"
    Else
        rst.FindFirst (strCriteria)
        If rst.NoMatch Then
            MsgBox "No entry found"
        Else
            Me.Bookmark = rst.Bookmark
        End If
    End If

Set rst = Nothing
DoCmd.GoToControl "txtProgID_find"

End Sub
Private Sub txtProgramName_AfterUpdate()

    Me.Filter = "[qryWeeklyAttendance].[ProgramNameDescription] like '*" & Me.txtProgramName.Value & "*'" & "" & "and [qryWeeklyAttendance].[FacilityName] ='" _
    & Me.cboFacility.Value & "'"
    Me.FilterOn = True

End Sub
