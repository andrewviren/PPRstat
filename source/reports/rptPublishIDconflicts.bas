Option Explicit
Private Sub Report_NoData(Cancel As Integer)
    DoCmd.OpenForm "frmWelcome"

    If MsgBox("There are no conflicts for the given criteria", vbOKOnly) = vbOK Then
        DoCmd.Close acForm, "rptPublishIDconflicts"
        
    End If

End Sub

Private Sub cmdConflictOK_Click()
    Dim strReason As String
    Dim strSQL As String
    Dim intPublishID As Integer
    Dim intConflictsWith As Integer
    Dim strUserInitials As String
    
    
    intPublishID = Me.txtPublishID
    intConflictsWith = Me.txtConflictsWith
    strUserInitials = Forms!frmWelcome.cmbUserName.Value
    
    strReason = InputBox("Why is this conflict ok?", "Confirming Conflict Information", "I called the rec leader and she knows.")
    strSQL = _
        "INSERT INTO [tblPermitConflictExceptions] " & _
        "(PublishID, ConflictWith, Notes, InputBy) VALUES" & _
        "(" & intPublishID & ", " & intConflictsWith & ", '" & strReason & "', " & "'" & strUserInitials & "'" & ");"
        
    DoCmd.RunSQL strSQL
    
    DoCmd.Requery
    
    
End Sub

Private Sub Report_Close()
    DoCmd.OpenForm "frmWelcome"
End Sub
